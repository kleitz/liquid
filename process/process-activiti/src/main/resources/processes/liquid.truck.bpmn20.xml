        <omgdi:waypoint x="1057.0" y="79.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow583" id="BPMNEdge_flow583">
        <omgdi:waypoint x="77.0" y="79.0"></omgdi:waypoint>
        <omgdi:waypoint x="77.0" y="409.0"></omgdi:waypoint>
        <omgdi:waypoint x="170.0" y="409.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow584" id="BPMNEdge_flow584">
        <omgdi:waypoint x="220.0" y="379.0"></omgdi:waypoint>
        <omgdi:waypoint x="220.0" y="90.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow586" id="BPMNEdge_flow586">
        <omgdi:waypoint x="800.0" y="61.0"></omgdi:waypoint>
        <omgdi:waypoint x="890.0" y="61.0"></omgdi:waypoint>
        <omgdi:waypoint x="890.0" y="186.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow587" id="BPMNEdge_flow587">
        <omgdi:waypoint x="590.0" y="207.0"></omgdi:waypoint>
        <omgdi:waypoint x="870.0" y="206.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</definitions>